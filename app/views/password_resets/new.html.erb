<% provide(:title, "Forgot password") %>
<h3>Reset Your Password</h3>

  <div class="field">
    <%= form_for(:password_reset, url: password_resets_path) do |f| %>
    <%= label_tag :email %><br>
    <%= f.email_field :email, class: 'form-control' %><br>
    <br>
    <div class="actions">
      <%= submit_tag "Submit" %>
      <%= link_to "Register", signup_path%>
      <% end %>
    </div>
  </div>










